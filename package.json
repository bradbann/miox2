{
  "name": "miox2",
  "version": "0.0.1",
  "description": "miox next web tool",
  "main": "build/index.js",
  "scripts": {
    "dev": "NODE_ENV=development node_modules/.bin/webpack-dev-server --config miox/build/webpack.config.js --host 0.0.0.0 --port 8090 --progress --colors --inline --hot --display-error-details --content-base miox/src/",
    "git": "git add . && git commit -am 'update' && git push",
    "build": "rimraf ./miox/release/ && NODE_ENV=production webpack --config miox/build/webpack.config.js -p",
    "build-engine": "NODE_ENV=production webpack --config miox-vue-engine/webpack.config.js -p",
    "build-router": "NODE_ENV=production webpack --config miox-router/webpack.config.js -p",
    "build-animate": "NODE_ENV=production webpack --config miox-animate/webpack.config.js -p"
  },
  "files": [
    "build",
    "src/css"
  ],
  "author": "沈赟杰",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "co": "^4.6.0",
    "css-loader": "^0.23.1",
    "domready": "^1.0.8",
    "es6-promise": "^3.2.1",
    "events": "^1.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "fastclick": "^1.0.6",
    "history": "^3.0.0",
    "html-webpack-plugin": "^2.22.0",
    "is-class": "0.0.4",
    "is-generator-function": "^1.0.3",
    "node-sass": "^3.8.0",
    "object-assign": "^4.1.0",
    "path-to-regexp": "^1.5.3",
    "postcss-loader": "^0.9.1",
    "querystring": "^0.2.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "setimmediate": "^1.0.4",
    "unique-array": "^1.0.1",
    "url": "^0.11.0",
    "vue": "^1.0.26",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
